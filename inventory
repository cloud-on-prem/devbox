[dev-box]
dev-box ansible_ssh_host=127.0.0.1 ansible_ssh_port=2222 ansible_ssh_user=vagrant user_name=vagrant workspace_dir=/work-space ansible_ssh_private_key_file=./.vagrant/machines/default/virtualbox/private_key logo=

[cloud-box]
cloud-box user_name=ubuntu ansible_ssh_host=<remote-machine-ip> ansible_ssh_port=22 ansible_ssh_user=ubuntu workspace_dir=/work-space ansible_ssh_private_key_file=/path/to/private_key workspace_dir=/work-space logo=

[mac]
mac ansible_connection=local user_name=prem workspace_dir=/work-space logo=
